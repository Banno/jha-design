<div class="thumbnail{{#each modifier}} thumbnail--{{.}}{{/each}}{{#each utility}} u-{{.}}{{/each}}" style="background-image: url({{url}});" {{#if isBackground}}role="image" aria-label="{{url}}" {{/if}}>
  {{#unless isBackground}}
    <img src="{{url}}" alt="{{alt}}">
  {{/unless}}
</div>
