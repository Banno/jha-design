{
    "color-case": "lower",
    "strip-spaces": true,
    "tab-size": 0,
    "element-case": "lower",
    "unitless-zero": true,
    "leading-zero": false,
    "color-shorthand": true,
    "vendor-prefix-align": false,
    "block-indent": 0,
    "always-semicolon": true,
    "space-before-colon": 0,
    "space-after-colon": 0,
    "space-before-opening-brace": 1,
    "space-after-opening-brace": 0,
    "space-before-closing-brace": 0,
    "space-before-selector-delimiter": 0,
    "space-after-selector-delimiter": 0,
    "space-between-declarations": 0,
    "lines-between-rulesets": 0,
    "quotes": "single",
    "space-before-combinator": 0,
    "space-after-combinator": 0,
    "remove-empty-rulesets": true,
    "eof-newline": true
}
